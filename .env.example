# Discord Bot設定
DISCORD_TOKEN=your_discord_bot_token_here

# Google Sheets設定
GOOGLE_SHEET_ID=your_google_sheet_id_here
# Google サービスアカウント認証ファイルのパス
GOOGLE_SERVICE_ACCOUNT_KEY=./google-credentials.json

# 通知設定
REMINDER_CHANNEL_ID=your_reminder_channel_id_here

# 任意設定
NODE_ENV=production

# Google Cloud Project設定（オプション）
# GOOGLE_CLOUD_PROJECT_ID=your_project_id_here

# タイムゾーン設定（デフォルト: Asia/Tokyo）
# TZ=Asia/Tokyo

# ログレベル設定（デフォルト: info）
# LOG_LEVEL=info
